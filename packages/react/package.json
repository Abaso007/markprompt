{
  "name": "@markprompt/react",
  "version": "0.3.0",
  "description": "A React component for adding GPT-4 powered search using the Markprompt API.",
  "main": "dist/index.cjs",
  "typings": "dist/index.d.ts",
  "module": "dist/markprompt.esm.js",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "files": [
    "README.md",
    "dist"
  ],
  "exports": {
    "import": "./dist/markprompt.esm.js",
    "require": "./dist/index.cjs",
    "types": "./dist/index.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/motifland/markprompt.git",
    "directory": "packages/react"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "./scripts/build.sh --external:react --external:react-dom",
    "clean": "rimraf ./dist"
  },
  "peerDependencies": {
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18"
  },
  "devDependencies": {
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "esbuild": "^0.17.8",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rimraf": "^3.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "client-only": "^0.0.1",
    "react-markdown": "^8.0.6",
    "remark-gfm": "^3.0.1",
    "@markprompt/core": "^0.3.0"
  }
}
